{
  "name": "pt-autorss",
  "version": "4.0.0",
  "description": "Auto fetch and download torrents even can auto load torrents.",
  "private": true,
  "scripts": {
    "start": "ts-node -T .",
    "lint": "tslint -p .",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "coverage": "nyc npm test"
  },
  "author": "huihuimoe",
  "license": "SATA",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "array-flat-polyfill": "1.0.0",
    "axios": "0.18.0",
    "dayjs": "1.8.10",
    "deluge-rpc": "1.0.3",
    "jsdom": "14.0.0",
    "lodash": "^4.17.11",
    "parse-torrent": "6.1.2",
    "transmission": "0.4.9",
    "ts-node": "8.0.3",
    "typescript": "3.3.3333",
    "xmlrpc": "1.3.2"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/jsdom": "12.2.3",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "5.2.6",
    "@types/node": "11.11.1",
    "@types/parse-torrent": "5.8.3",
    "@types/sinon": "7.0.9",
    "@types/xmlrpc": "1.3.5",
    "chai": "4.2.0",
    "husky": "1.3.1",
    "mocha": "6.0.2",
    "nyc": "13.3.0",
    "prettier": "1.16.4",
    "sinon": "7.2.7",
    "tslint": "5.13.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "renovate": {
    "extends": [
      "config:base"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huihuimoe/pt-autorss.git"
  },
  "bugs": {
    "url": "https://github.com/huihuimoe/pt-autorss/issues"
  },
  "homepage": "https://github.com/huihuimoe/pt-autorss#readme"
}
